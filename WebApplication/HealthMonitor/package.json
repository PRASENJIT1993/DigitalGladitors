{
  "name": "HealthMonitor",
  "version": "0.0.1",
  "description": "Sample project that is used to monitor the health",
  "scripts": {
    "clean": "rimraf dist",
    "clean-production": "rimraf dist-production",
    "prebuild": "npm install && npm run clean",
    "prebuild-production": "npm install && npm run clean-production",
    "start": "webpack-dev-server --env.mode WEB --config webpack.development.config.js --inline --hot --colors --progress --display-error-details --display-cached",
    "build-cordova": "webpack --env.mode MOBILE --config webpack.development.config.js --progress --profile --colors --display-error-details --display-cached",
    "postbuild-cordova": "xcopy .\\dist .\\www /h /i /k /e /r /y",
    "build": "webpack -p --env.mode WEB --config webpack.development.config.js --progress --profile --colors --display-error-details --display-cached",
    "build-production": "webpack -p --env.mode WEB --config webpack.production.config.js --progress --profile --colors --display-error-details --display-cached",
    "postbuild": "jar -cvf dist/HealthMonitor.war -C dist .",
    "postbuild-production": "jar -cvf dist-production/HealthMonitor.war -C dist-production ."
  },
  "licence": "UNLINCENSED",
  "author": "Health Monitoring Team",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.4",
    "babel-eslint": "^8.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "html-webpack-plugin": "^2.7.2",
    "react-hot-loader": "^1.3.0",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "copy-webpack-plugin": "3.0.1",
    "autoprefixer": "6.7.3",
    "babel-cli": "6.23.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.3.2",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.22.0",
    "babel-register": "6.23.0",
    "browser-sync": "2.18.8",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.16",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "enzyme": "2.7.1",
    "eslint": "3.15.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "eslint-watch": "3.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "isparta": "4.0.0",
    "mocha": "3.2.0",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.1",
    "open": "0.0.5",
    "postcss-loader": "1.3.1",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.4.2",
    "redux-immutable-state-invariant": "1.2.4",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "6.0.1",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0",
